{
  "name": "platzi-store",
  "version": "1.0.0",
  "description": "Proyecto para el curso de Jest",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --open --mode development",
    "format": "prettier --write '{*.js,src/**/*.{js,jsx}}'",
    "lint": "eslint src/",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gndx/platzi-store.git"
  },
  "keywords": [
    "React",
    "Webpack",
    "Babel",
    "JavaScript",
    "React-router",
    "Redux"
  ],
  "author": "Oscar Barajas <oscar@arepa.dev>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^5.2.7",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^1.6.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "style-loader": "^2.0.0",
    "stylus": "^0.55.0",
    "stylus-loader": "^4.3.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^4.8.0"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.4.1",
    "react-test-renderer": "^16.13.0",
    "webpack-dev-server": "^3.9.0"
  },
  "bugs": {
    "url": "https://github.com/gndx/platzi-store/issues"
  },
  "homepage": "https://github.com/gndx/platzi-store/#readme",
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/__test__/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(styl|css)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  }
}
